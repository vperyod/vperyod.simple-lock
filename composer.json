    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Vperyod\\SimpleLock\\": "src/"
        }
